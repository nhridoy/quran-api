{
    "no": 114,
    "name": "سُورَةُ النَّاسِ",
    "enName": "An-Naas",
    "enNameTranslation": "Mankind",
    "bnNameTranslation": "আন নাস",
    "revelationType": "Meccan",
    "numberOfAyahs": 6,
    "verses": [
        {
            "text": "قُلْ أَعُوذُ بِرَبِّ النَّاسِ",
            "bnText": "বলুন, আমি আশ্রয় গ্রহণ করিতেছি মানুষের পালনকর্তার",
            "enText": "Say, \"I seek refuge in the Lord of mankind,",
            "enTextTransliteration": "Qul a'uzu birabbin naas",
            "audioPrimary": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6231",
            "numberInSurah": 1
        },
        {
            "text": "مَلِكِ النَّاسِ",
            "bnText": "মানুষের অধিপতির",
            "enText": "The Sovereign of mankind.",
            "enTextTransliteration": "Malikin naas",
            "audioPrimary": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6232",
            "numberInSurah": 2
        },
        {
            "text": "إِلَٰهِ النَّاسِ",
            "bnText": "মানুষের মা’বুদের",
            "enText": "The God of mankind,",
            "enTextTransliteration": "Ilaahin naas",
            "audioPrimary": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6233",
            "numberInSurah": 3
        },
        {
            "text": "مِن شَرِّ الْوَسْوَاسِ الْخَنَّاسِ",
            "bnText": "তার অনিষ্ট থেকে, যে কুমন্ত্রণা দেয় ও আত্নগোপন করে",
            "enText": "From the evil of the retreating whisperer -",
            "enTextTransliteration": "Min sharril was waasil khannaas",
            "audioPrimary": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6234",
            "numberInSurah": 4
        },
        {
            "text": "الَّذِي يُوَسْوِسُ فِي صُدُورِ النَّاسِ",
            "bnText": "যে কুমন্ত্রণা দেয় মানুষের অন্তরে",
            "enText": "Who whispers [evil] into the breasts of mankind -",
            "enTextTransliteration": "Al lazee yuwas wisu fee sudoorin naas",
            "audioPrimary": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6235",
            "numberInSurah": 5
        },
        {
            "text": "مِنَ الْجِنَّةِ وَالنَّاسِ",
            "bnText": "জ্বিনের মধ্য থেকে অথবা মানুষের মধ্য থেকে।",
            "enText": "From among the jinn and mankind.\"",
            "enTextTransliteration": "Minal jinnati wan naas",
            "audioPrimary": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6236",
            "numberInSurah": 6
        }
    ]
}